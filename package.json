{
  "name": "codeclimate-test-reporter",
  "version": "1.0.0",
  "description": "Send test coverage results to code climate",
  "main": "index.js",
  "scripts": {
    "lint": "eslint index.js",
    "package": "ncc build index.js -o dist",
    "test": "jest",
    "git-precommit": "npm run package",
    "git-add": "git add dist"
  },
  "precommit": [
    "git-precommit",
    "git-add"
  ],
  "jest": {
    "verbose": false,
    "collectCoverage": true,
    "coverageReporters": [
      "clover",
      "lcov"
    ],
    "setupFilesAfterEnv": [
      "jest-extended"
    ]
  },
  "keywords": [
    "GitHub",
    "Actions",
    "CodeClimate"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aktions/codeclimate-test-reporter.git"
  },
  "author": "alexkappa <me@alexkappa.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aktions/codeclimate-test-reporter/issues"
  },
  "homepage": "https://github.com/aktions/codeclimate-test-reporter#readme",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4",
    "@actions/github": "^1.1.0",
    "@actions/io": "^1.0.2",
    "@actions/tool-cache": "^1.6.1"
  },
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^6.8.0",
    "jest": "^24.9.0",
    "jest-extended": "^0.11.5",
    "pre-commit": "^1.2.2"
  }
}
